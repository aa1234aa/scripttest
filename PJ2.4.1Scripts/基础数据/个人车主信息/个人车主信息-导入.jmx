        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="1.2个人车主信息-生成导入文件" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import org.apache.poi.hssf.usermodel.HSSFWorkbook;
import org.apache.poi.ss.usermodel.Row;
import org.apache.poi.ss.usermodel.Sheet;
import org.apache.poi.ss.usermodel.Workbook;

           Workbook wb=new HSSFWorkbook();  
		 Sheet sheet0=wb.createSheet(&quot;Sheet0&quot;); 
		 
		 Row row0=sheet0.createRow(0);  
		 row0.createCell(0).setCellValue(&quot;姓名&quot;);  
		 row0.createCell(1).setCellValue(&quot;性别&quot;); 
		 row0.createCell(2).setCellValue(&quot;联系电话&quot;);  
		 row0.createCell(3).setCellValue(&quot;联系地址&quot;);  
		 row0.createCell(4).setCellValue(&quot;邮箱&quot;);  
		 row0.createCell(5).setCellValue(&quot;证件类型&quot;);  
		 row0.createCell(6).setCellValue(&quot;证件号码&quot;);
		 row0.createCell(7).setCellValue(&quot;所有人证件地址&quot;);    
		 
		 Row row1=sheet0.createRow(1);
		 row1.createCell(0).setCellValue(&quot;bitnei车主${Random1}&quot;);
		 row1.createCell(1).setCellValue(&quot;女&quot;);
		 row1.createCell(2).setCellValue(&quot;139${Random}&quot;);
		 row1.createCell(3).setCellValue(&quot;珠海香洲区唐家湾${Random1}&quot;);
		 row1.createCell(4).setCellValue(&quot;139${Random}@qq.com&quot;);
		 row1.createCell(5).setCellValue(&quot;居民身份证&quot;);
		 row1.createCell(6).setCellValue(&quot;44188219890817${Random1}&quot;);
		 row1.createCell(7).setCellValue(&quot;珠海市香洲区唐家湾${Random1}&quot;);

		 
		 FileOutputStream fileout = null;
		 String filePath = vars.get(&quot;PROJECT_PATH&quot;) +  &quot;/Resources/upload/个人车主信息导入文件.xls&quot;;
		try {
			fileout = new FileOutputStream(filePath);
		} catch (FileNotFoundException e1) {
			e1.printStackTrace();
		}
		 try {
			wb.write(fileout);
		} catch (IOException e) {
			e.printStackTrace();
		}
		 try {
			fileout.close();
		} catch (IOException e) {
			e.printStackTrace();
		}

		vars.put(&quot;filePath&quot;, filePath);
		 
		</stringProp>
