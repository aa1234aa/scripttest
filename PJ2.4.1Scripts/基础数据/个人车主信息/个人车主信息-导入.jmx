            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.sql.*;
import com.bitnei.util.*;

			
		String sql=&quot; SELECT id FROM sys_veh_owner where owner_name=&apos;bitnei车主${Random1}&apos;&quot;;
	
		Connection conn=null;
		Statement stmt=null;
		ResultSet rs = null ;	
		conn=DbUtil.getCon();  
	     stmt=conn.createStatement();
		rs = stmt.executeQuery(sql) ;
		
		while(rs.next()){
			String id=rs.getString(&quot;id&quot;);
			vars.put(&quot;veh_owner_id&quot;,id);
		}	
			
	      DbUtil.closeCon(conn);</stringProp>
